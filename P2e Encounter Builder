#set values for any variable needed
def user_input():
	verify = False
	global threatLevel
	global partyNumber
	global partyLevelModify
	global enemies #remember to global anything used by multiple blocks?
	global enemiesPartyVisual
	global partyLevel
	global terrain

	print('party level?')
	partyLevel = float(input())
	print('how many party members?')
	partyNumber = int(input())
	while verify == False:	
		check = (1, 2, 3, 4, 5)
		print("what threat level do you want? 1 is trivial, 2 is low, 3 is moderate, 4 is severe, 5 is extreme ")
		threatLevel = int(input())
		if threatLevel in check:
			verify = True	
	print("enter a terrain")
	verify = False
	while verify == False:
		print("What terrain?")
		terrain = input()
		if terrain in enviroments:
			verify = True
	PartyLevelModify = float(1.0) #needs to be float

#get the xp budget to spend on monsters
def budgeter():
	if (threatLevel == 1):
		xpBudget = 40
	elif (threatLevel == 2):
		xpBudget = 60
	elif (threatLevel ==3):
		xpBudget = 80
	elif (threatLevel == 4):
		xpBudget = 120
	else:
		xpBudget = 160
	partyNumberModify = (partyNumber / 4)
	if (threatLevel == 2):
		partyNumberModify = (partyNumber - 1) / 3
	xpBudget = (int((xpBudget / 10) * partyNumberModify)) * 10
	return(xpBudget)
	
	#spend budget on monster levels
def partyComp():
	x = int(budgeter())
	xPerm = x #if reference is needed
	x = x / 5
	y = partyLevel
	import random
	enemiesParty = []
	enemiesPartyVisual = []
	enemies = [2, 3, 4, 6, 8, 12, 16, 24, 32]
	number = True
	while number == True:
		monster = random.choice(enemies)
		x = x - int(monster)		
		if (x >= 2):
			enemiesParty.append(str(monster))
			number = True
		elif (x <0):
			x = x + monster	#refunds if negitive
		else:
			number = False
			enemiesParty.append(str(monster))
			for x in enemiesParty:
				if int(x) == 2:
					enemiesPartyVisual.append("PL-4")
				if int(x) == 3:
					enemiesPartyVisual.append("PL-3")
				if int(x) == 4:
					enemiesPartyVisual.append("PL-2")
				if int(x) == 6:
					enemiesPartyVisual.append("PL-1")
				if int(x) == 8:
					enemiesPartyVisual.append("PL")
				if int(x) == 12:
					enemiesPartyVisual.append("PL+1")
				if int(x) == 16:
					enemiesPartyVisual.append("PL+2")
				if int(x) == 24:
					enemiesPartyVisual.append("PL+3")
				if int(x) == 32:
					enemiesPartyVisual.append("PL+4")
			list.sort(enemiesPartyVisual)
			return enemiesPartyVisual
			#done generating party

#get lootvalue
def lootGen(a, b):
	ratioList = (0, 0.07, 0.1, 0.15, 0.2)
	y = ratioList[a - 1]
	gTotal = calculateGP(b)
	return (y * gTotal)
	
def calculateGP(x):
	if x == 1:
		return 175
	else:
		return 2.5 * (x ** 3) - 10 * (x **2) + 150 * x + 50
  
 #retrieve all monster class objects to sort from
def monSort():
	allMons = []
	allVariables = dict(globals(), **locals())
	for name, var in allVariables.items():
		if isinstance(var, monster):
			allMons.append(var)
	return allMons

#changes list of player level modified list to true monster level list
def monChoices():
	global partyLevel
	global typeMon
	y = partyLevel
	monLevelList = []
	monList = partyComp()
	for x in monList:
		if x == "PL-4":
			monLevelList.append(-4)
		if x == "PL-3":
			monLevelList.append(-3)
		if x == "PL-2":
			monLevelList.append(-2)
		if x == "PL-1":
			monLevelList.append(-1)
		if x == "PL":
			monLevelList.append(0)
		if x == "PL+1":
			monLevelList.append(1)
		if x == "PL+2":
			monLevelList.append(2)
		if x == "PL+3":
			monLevelList.append(3)
		if x == "PL+4":
			monLevelList.append(4)
	tempList = [x + int(partyLevel) for x in monLevelList]
	return tempList
	
#sort monsters based on common filters
def monListFilter(y, a, b): # cant use for CR?
	tempGroup = []
	for x in y:
		if getattr(x, a) == b:
			tempGroup.append(x)
		
	return tempGroup
	

	#make the list to pull from
def monListMaker():
	global terrain
	global partyLevel
	typeOfMon = None 
	tempList = []
	primeList = []	
	secondaryList = []
	filteredByCR = []
	filteredByType = []
	filteredByTerrain = []
	allMons = monSort()
	finalList = []
	for CR in allMons:
		if (int(partyLevel - 4) < int(getattr(CR, "CR")) < int(partyLevel + 4)):
			if CR not in filteredByCR:
				filteredByCR.append(CR)
	list1 = monListFilter(filteredByCR, "terrain", terrain)
	list2 = monListFilter(filteredByCR, "terrain", "any")
	filteredByTerrain = list1 + list2	
	
	for x in filteredByTerrain:
		if terrain == "aquatic":
			if getattr(x, "terrain") == "any":
				filteredByTerrain.remove(x)
	 
	return filteredByTerrain

#pick one to set type with
def monOnePicker():
	global levelListChoices 
	tempList = []
	levelListChoices = monChoices()
	monList = monListMaker()
	tempVar = levelListChoices.pop(0)
	for x in monList:
		if int(getattr(x, "CR")) == int(tempVar):
			tempList.append(x)	
	tempMon = random.choice(tempList)
	return tempMon
	
def main():
	global levelListChoices
	global partyLevel
	global threatLevel
	user_input()
	firstMon = monOnePicker()
	finalList= []
	finalList.append(firstMon)
	trueList = []
	typeMon = getattr(firstMon, "type")
	filteredByTerrain = []
	filteredByTerrain = monListMaker()
	trueList = monListFilter(filteredByTerrain, "type", typeMon)
	for x in trueList:
		for y in levelListChoices:
			if getattr(x, "CR") == y:
				finalList.append(x)
				levelListChoices.remove(y)
			else:
				pass
		
		
	for x in finalList:
	 print(x.__dict__)
	gold = lootGen(threatLevel, partyLevel)
	print("gp total: ", gold)
while 1 == 1:
	main() #to test with different inputs



	

